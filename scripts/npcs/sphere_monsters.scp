   IF (<ACT.TYPE>==t_corpse)
      IF !(<ACT.TAG0.PlagueEaten>)
         MAXHITS += 200
         HITS += 200
         TAG.CorpsesConsumed += 1
         ACT.TAG.PlagueEaten = 1
         EMOTE eat a corpse
      ENDIF
   ENDIF
ON=@GetHit
   //Spawn Creatures Attack (plague spawn)
   IF !(<R10>)
      SERV.NEWNPC c_plague_spawn
      NEW.P=<P>
      NEW.HOME=<SRC.P>
      NEW.HOMEDIST=10
      NEW.ATTACK <SRC>
      NEW.f_NPCRemovalTimer 1200
   ENDIF

